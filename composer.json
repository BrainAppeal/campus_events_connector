{
    "name": "brainappeal/campus_events_connector",
    "type": "typo3-cms-extension",
    "description" : "Brain Appeal Campus Events connector for TYPO3",
    "license": [
        "GPL-2"
    ],
    "homepage": "https://www.campus-events.com/",
    "authors": [
        {
            "name": "Brain Appeal Team",
            "email": "info@brain-appeal.com"
        },
        {
            "name": "Joshua Billert",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "~7.6.0|~8.7.0|~9.5.0",
        "guzzlehttp/guzzle": "~6.3.0"
    },
    "autoload": {
        "psr-4": {
            "BrainAppeal\\CampusEventsConnector\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrainAppeal\\CampusEventsConnector\\Tests\\": "Tests"
        }
    },
    "replace": {
        "campus_events_connector": "self.version",
        "typo3-ter/campus_events_connector": "self.version"
    }
}
